<!doctype html>
<html lang="{{ guest.system_locale|replace({'_': '-'}) }}"{% if guest.system_locale in ['ar_EG', 'ar_SA', 'he_IL'] %} {{ 'dir="rtl"'| raw}}{% endif %}>
   <head>
      <meta charset="utf-8"/>
      <meta name="viewport" content="width=device-width, initial-scale=1, viewport-fit=cover"/>
      <meta http-equiv="X-UA-Compatible" content="ie=edge"/>
      <title>{% block meta_title %}{{ settings.meta_title }}{% endblock %}</title>
      <meta name="msapplication-TileColor" content=""/>
      <meta name="theme-color" content=""/>
      <meta name="apple-mobile-web-app-status-bar-style" content="black-translucent"/>
      <meta name="apple-mobile-web-app-capable" content="yes"/>
      <meta name="mobile-web-app-capable" content="yes"/>
      <meta name="HandheldFriendly" content="True"/>
      <meta name="MobileOptimized" content="320"/>
      <link rel="icon" href="{{ guest.system_company.favicon_url }}" type="image/x-icon"/>
      <link rel="shortcut icon" href="{{ guest.system_company.favicon_url }}" type="image/x-icon"/>
      <meta name="description" content="{% block meta_description %}{{ settings.meta_description }}{% endblock %}"/>
      <meta property="bb:url" content="{{ constant('SYSTEM_URL') }}">
      <meta property="bb:client_area" content="{{ '/'|link }}">
      <meta name="csrf-token" content="{{ CSRFToken }}">
      <meta name="robots" content="{{ settings.meta_robots }}">
      <meta name="author" content="{{ settings.meta_author }}">
      <meta name="generator" content="FOSSBilling {{ guest.system_version }}">
      <meta name="canonical" content="{{ constant('SYSTEM_URL') }}">
      {% block opengraph %}{% endblock %}
      <!-- CSS files -->
      <link href="{% if guest.system_locale in ['ar_EG', 'ar_SA', 'he_IL'] %}{{ 'css/tabler.rtl.min.css' | asset_url }}{% else %}{{ 'css/tabler.min.css' | asset_url }}{% endif %}" rel="stylesheet"/>
      <link href="{% if guest.system_locale in ['ar_EG', 'ar_SA', 'he_IL'] %}{{ 'css/tabler-flags.rtl.min.css' | asset_url }}{% else %}{{ 'css/tabler-flags.min.css' | asset_url }}{% endif %}" rel="stylesheet"/>
      <link href="{% if guest.system_locale in ['ar_EG', 'ar_SA', 'he_IL'] %}{{ 'css/tabler-payments.rtl.min.css' | asset_url }}{% else %}{{ 'css/tabler-payments.min.css' | asset_url }}{% endif %}" rel="stylesheet"/>
      <link href="{% if guest.system_locale in ['ar_EG', 'ar_SA', 'he_IL'] %}{{ 'css/tabler-vendors.rtl.min.css' | asset_url }}{% else %}{{ 'css/tabler-vendors.min.css' | asset_url }}{% endif %}" rel="stylesheet"/>
	  <link href="{{ 'css/custom.css' | asset_url }}" rel="stylesheet"/>
      <script src="{{ "Api/API.js?v=#{guest.system_version}" | library_url }}"></script>
      <script src="{{ 'js/jquery-3.7.0.min.js' | asset_url }}"></script>
      <script src="{{ 'js/bb-jquery.js' | asset_url }}"></script>
      <style>
         @import url('https://rsms.me/inter/inter.css');
         :root {
         --tblr-font-sans-serif: 'Inter Var', -apple-system, BlinkMacSystemFont, San Francisco, Segoe UI, Roboto, Helvetica Neue, sans-serif;
         }
         body {
         font-feature-settings: "cv03", "cv04", "cv11";
         }
      </style>
      {% block head %}{% endblock %}
      {% block js %}{% endblock %}
   </head>
   <body>
      {% if guest.extension_is_on({ "mod": "cookieconsent" }) %}
      {% include 'mod_cookieconsent_index.html.twig' ignore missing%}
      {% endif %}
      {% block body %}
      <script>
         if (localStorage.getItem('theme') === 'dark') {
            document.body.setAttribute("data-bs-theme", localStorage.getItem('theme'));
         } else {
            document.body.removeAttribute("data-bs-theme");
         }
      </script>
      {% if not client and settings.require_login%}
      <script>
         $(function() {
             bb.redirect('{{ "login"|link }}');
         });
      </script>
      {% endif %}
      {% if client %}
      {% set profile = client.profile_get %}
      {% endif %}
      {% set company = guest.system_company %}
      <div class="page">
         <!-- Navbar -->
         <div class="sticky-top">
            {% if settings.announcement_bar_enabled %}
            <header class="navbar navbar-expand-md d-print-none bg-{% if settings.announcement_bar_color == 1 %}primary{% elseif settings.announcement_bar_color == 2 %}purple{% elseif settings.announcement_bar_color == 3 %}red{% elseif settings.announcement_bar_color == 4 %}orange{% elseif settings.announcement_bar_color == 5 %}yellow{% else %}green{% endif %} text-primary-fg border-bottom-0">
               <div class="container-xl ml-3">
                  <strong>{{ settings.announcement_bar_text }}</strong>
               </div>
            </header>
            {% endif %}
            <header class="navbar navbar-expand-md d-print-none{% if settings.announcement_bar_enabled %} border-top-0{% endif %}">
               <div class="container-xl">
                  <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbar-menu" aria-controls="navbar-menu" aria-expanded="false" aria-label="Toggle navigation">
                  <span class="navbar-toggler-icon"></span>
                  </button>
				  {% if settings.show_company_logo %}
                  <h1 class="navbar-brand navbar-brand-autodark d-none-navbar-horizontal pe-0 pe-md-3">
                     <a href="{{'/'|link }}">
                     <img src="{{company.logo_url}}" alt="{{company.name}}" class="navbar-brand-image">
                     </a>
                  </h1>
				  {% endif %}
				  {% if settings.show_company_name %}
				  <p class="mt-3 text-info">{{company.name}}</p>
				  {% endif %}				  
                  <div class="navbar-nav flex-row order-md-last">
                     <div class="nav-item d-none d-md-flex me-3">
                        <div class="btn-list">
                           {% set languages = guest.extension_languages(true) %}
                           {% if languages|length > 1 %}
                           {% set currentLang = guest.system_locale %}
                           {% set countryCode = currentLang | slice(3, 2) %}
                           <div class="dropdown">
                              <a href="#" class="btn dropdown-toggle" data-bs-toggle="dropdown"><span class="flag flag-country-{{ countryCode | lower }}"></span>&nbsp;{% for lang in languages %}
                              {% if lang.locale == currentLang %}
                              {{ lang.title }}
                              {% endif %}
                              {% endfor %}</a>
                              <div class="dropdown-menu">
                                 {% for lang in languages %}
                                 {% set countryCode = lang.locale | slice(3, 2) %}
                                 {% if lang != currentLang %}
                                 <a href="javascript:;" class="dropdown-item language_selector" data-language-code="{{ lang.locale }}"><span class="flag flag-country-{{ countryCode | lower }}"></span>&nbsp;{{ lang.title }}</a>
                                 {% endif %}
                                 {% endfor %}
                              </div>
                           </div>
                           {% endif %}
                           {% if not client %}
                           <a href="{{ 'login'|link }}" class="btn btn-secondary">
                              <svg xmlns="http://www.w3.org/2000/svg" class="icon" width="24" height="24" viewBox="0 0 24 24" stroke-width="2" stroke="currentColor" fill="none" stroke-linecap="round" stroke-linejoin="round">
                                 <path stroke="none" d="M0 0h24v24H0z" fill="none"></path>
                                 <path d="M14 8v-2a2 2 0 0 0 -2 -2h-7a2 2 0 0 0 -2 2v12a2 2 0 0 0 2 2h7a2 2 0 0 0 2 -2v-2"></path>
                                 <path d="M20 12h-13l3 -3m0 6l-3 -3"></path>
                              </svg>
                              {{ 'Login'|trans }}
                           </a>
                           <a href="{{ 'signup'|link }}" class="btn btn-primary">
                              <svg xmlns="http://www.w3.org/2000/svg" class="icon" width="24" height="24" viewBox="0 0 24 24" stroke-width="2" stroke="currentColor" fill="none" stroke-linecap="round" stroke-linejoin="round">
                                 <path stroke="none" d="M0 0h24v24H0z" fill="none"></path>
                                 <path d="M8 7a4 4 0 1 0 8 0a4 4 0 0 0 -8 0"></path>
                                 <path d="M16 19h6"></path>
                                 <path d="M19 16v6"></path>
                                 <path d="M6 21v-2a4 4 0 0 1 4 -4h4"></path>
                              </svg>
                              {{ 'Register'|trans }}
                           </a>
                           {% endif %}
                        </div>
                     </div>
                     <div class="d-none d-md-flex me-3">
                        <a href="?theme=dark" class="nav-link px-0 hide-theme-dark js-theme-toggler" title="Enable dark mode" data-bs-toggle="tooltip"
                        data-bs-placement="bottom">
                           <svg xmlns="http://www.w3.org/2000/svg" class="icon" width="24" height="24" viewBox="0 0 24 24" stroke-width="2" stroke="currentColor" fill="none" stroke-linecap="round" stroke-linejoin="round">
                           <path stroke="none" d="M0 0h24v24H0z" fill="none"/>
                           <path d="M12 3c.132 0 .263 0 .393 0a7.5 7.5 0 0 0 7.92 12.446a9 9 0 1 1 -8.313 -12.454z" />
                           </svg>
                        </a>
                        <a href="?theme=light" class="nav-link px-0 hide-theme-light js-theme-toggler" title="Enable light mode" data-bs-toggle="tooltip"
                        data-bs-placement="bottom">
                           <svg xmlns="http://www.w3.org/2000/svg" class="icon" width="24" height="24" viewBox="0 0 24 24" stroke-width="2" stroke="currentColor" fill="none" stroke-linecap="round" stroke-linejoin="round">
                           <path stroke="none" d="M0 0h24v24H0z" fill="none"/>
                           <path d="M12 12m-4 0a4 4 0 1 0 8 0a4 4 0 1 0 -8 0" />
                           <path d="M3 12h1m8 -9v1m8 8h1m-9 8v1m-6.4 -15.4l.7 .7m12.1 -.7l-.7 .7m0 11.4l.7 .7m-12.1 -.7l-.7 .7" />
                           </svg>
                        </a>
                     </div>
                     {% if client %}
                     <div class="nav-item dropdown">
                        <a href="#" class="nav-link d-flex lh-1 text-reset p-0" data-bs-toggle="dropdown" aria-label="Open user menu">
                           <span class="avatar avatar-sm" style="background-image: url({{ profile.email|gravatar(60) }})"></span>
                           <div class="d-none d-xl-block ps-2">
                              <div>Welcome, {{profile.first_name}}</div>
                              <div class="mt-1 small text-muted">Balance: {{ profile.balance | money(profile.currency) }}</div>
                           </div>
                        </a>
                        <div class="dropdown-menu dropdown-menu-end dropdown-menu-arrow">
                           <a href="{{ 'client/profile' | link}}" class="dropdown-item">{{ 'Profile'|trans }}</a>
                           <a href="{{ '/email'|link }}" class="dropdown-item">{{ 'Emails'|trans }}</a>
                           <a href="{{ 'client/logout' | link}}" class="dropdown-item">{{ 'Sign out'|trans }}</a>
                        </div>
                     </div>
                     {% endif %}
                  </div>
               </div>
            </header>
            <header class="navbar-expand-md d-print-none">
               <div class="collapse navbar-collapse" id="navbar-menu">
                  <div class="navbar">
                     <div class="container-xl">
                        <ul class="navbar-nav">
						   {% if settings.main_menu_home %}
